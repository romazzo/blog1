{% extends "@App/Layout/layout-two-col-8-4.html.twig" %}

{% block Post %}

    <h3>Edit</h3>


    <div class="post-edit">
        <div class="post">
            {{ form_start(form) }}
                {{ form_errors(form) }}
                {{ form_row(form.title) }}
                {{ form_row(form.description) }}
                {{ form_row(form.createdAt) }}
                {{ form_row(form.submit, { 'label': 'Save' }) }}
            {{ form_end(form) }}

        </div>
    </div>
    <div class="post-link"><a href="/">back</a></div>

{% endblock %}